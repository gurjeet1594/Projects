class Divisible {
public static void main (String []args) {
	int number;
	number = 40;
	
	if((number%3==0) && (number%5==0)) {
		System.out.println("fizzbuzz");
	}

	else if (number%3==0) {
		System.out.println("Fizz");
	}
	
	else if  (number%5==0) {
		System.out.println("buzz");
	}
  
}
}